{
  "builds": [
    {
      "src": "front_end/package.json",
      "use": "@vercel/node",
      "config": {
        "buildCommand": "cd front_end && npm install && npm run build",
        "outputDirectory": "front_end/build"
      }
    }
  ],
  "routes": [
    {
      "src": "/(.*)",
      "dest": "front_end/build/$1"
    }
  ]
}